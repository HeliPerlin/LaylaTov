<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.laylatov">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.LaylaTov">
        <activity
            android:name=".chooseInfoType"
            android:label="@string/title_activity_choose_info_type"
            android:theme="@style/Theme.LaylaTov.NoActionBar"></activity>
        <activity android:name=".WelcomeScreen" />
        <activity android:name=".UsefulInfoClient" />
        <activity android:name=".VideoScreen" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>